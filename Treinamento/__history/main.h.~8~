#include <16F628A.h>
#device ADC=16
#FUSES PUT                      //Power Up Timer
#FUSES NOLVP                    //No low voltage prgming, B3(PIC16) or B5(PIC18) used for I/O

#use delay(internal=8MHz)
#use FIXED_IO( A_outputs=PIN_A0 )


#define LAMPADA   PIN_A0
#BYTE REG_INTCON = 0x0B

